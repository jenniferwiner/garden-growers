{{#each feedData}}
<div class="text-center" style="margin-top: 40px">
    <div class="col-md-6">
      <div class="panel panel-default clicable feedPanel">
        <div class="panel-body">
          <div class="top-img">
            <img class="top-img" src="{{this.photo}}">
          </div>
        <div class="top-content">
            <div class="question">
                <p class="x-large"><b>{{this.common_name}} | {{this.scientific_name}}</b></p>
            </div>
        <div class="made-by">
            <p class="text-muted">
                <b class="cool-font">
                    <span>{{this.description}}</span>
                </b>
            </p>
        </div><br>
        <p class="text-center xx-large avg">
          <b><a href="/feed/{{this.id}}"><button class="hvr-grow-shadow gardenView" type="button" name="button">View {{this.garden_name}}'s garden!</button></a><br></b>
        </p>
        </div>
      </div>
    </div>
  </div>
</div>
{{/each}}
